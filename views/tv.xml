<!-- VIEW MEDIAMETRIE ENSEMBLE -->

        <record model="ir.ui.view" id="mm_ensemble_form_view">
            <field name="name">MM Ensemble</field>
            <field name="model">mm.ensemble</field>
            <field name="arch" type="xml">
                <form string="Penetration TV">
                    <sheet>
                        <group string="MEDIAMETRIE TV">
                            <field name="jour"/>
                            <field name="name"/>
                            <field name="slotens"/>
                            <field name="tauxens"/>
                            <field name="mois"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

<!-- VIEW MEDIAMETRIE ENSEMBLE GRAPH VIEW-->
        <record model="ir.ui.view" id="mm_ensemble_graph_view">
            <field name="name">MM Ensemble Graph</field>
            <field name="model">mm.ensemble</field>
            <field name="arch" type="xml">
                <graph string="Analyse">
                    <field name="slotens" type="row"/>
                    <field name="name" type="row"/>
                    <field name="tauxens" type="measure"/>
                </graph>
            </field>
        </record>

<!-- VIEW SEARCH MEDIAMETRIE ENSEMBLE -->

        <record model="ir.ui.view" id="mm_ensemble_search_view">
            <field name="name">MM Ensemble Search</field>
            <field name="model">mm.ensemble</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="slotens"/>
                    <field name="tauxens"/>
                    <field name="jour"/>
                    <field name="mois"/>
                    <filter name="sanstempsmort" string="PAS 0h00 à 5h00" domain="[('slotens','!=','0h00-0h15'),('slotens','!=','0h15-0h30'),('slotens','!=','0h30-0h45'),('slotens','!=','0h45-1h00'),('slotens','!=','1h00-1h15'),('slotens','!=','1h15-1h30'),('slotens','!=','1h30-1h45'),('slotens','!=','1h45-2h00'),('slotens','!=','2h00-2h15'),('slotens','!=','2h15-2h30'),('slotens','!=','2h30-2h45'),('slotens','!=','2h45-3h00'),('slotens','!=','3h00-3h15'),('slotens','!=','3h15-3h30'),('slotens','!=','3h30-3h45'),('slotens','!=','3h45-4h00'),('slotens','!=','4h00-4h15'),('slotens','!=','4h15-4h30'),('slotens','!=','4h30-4h45'),('slotens','!=','4h45-5h00')]"/>
                    <separator/>
                    <filter name="lundi" string="LUNDI" domain="[('jour','=','lundi')]"/>
                    <filter name="mardi" string="MARDI" domain="[('jour','=','mardi')]"/>
                    <filter name="mercredi" string="MERCREDI" domain="[('jour','=','mercredi')]"/>
                    <filter name="jeudi" string="JEUDI" domain="[('jour','=','jeudi')]"/>
                    <filter name="vendredi" string="VENDREDI" domain="[('jour','=','vendredi')]"/>
                    <filter name="samedi" string="SAMEDI" domain="[('jour','=','samedi')]"/>
                    <filter name="dimanche" string="DIMANCHE" domain="[('jour','=','dimanche')]"/>
                </search>
            </field>
        </record>

<!-- VIEW MEDIAMETRIE ENSEMBLE tree/list view -->
        <record model="ir.ui.view" id="mm_ensemble_tree_view">
            <field name="name">MM Ensemble</field>
            <field name="model">mm.ensemble</field>
            <field name="arch" type="xml">
                <tree string="Penetration TV">
                    <field name="jour"/>
                    <field name="name"/>
                    <field name="slotens"/>
                    <field name="tauxens"/>
                    <field name="mois"/>
