<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
<!-- VIEW FORM LISTE TELE -->

        <record model="ir.ui.view" id="liste_tele_form_view">
            <field name="name">listetele.form</field>
            <field name="model">liste.tv</field>
            <field name="arch" type="xml">
                <form string="Liste TV">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="dureetv1"/>
                            <field name="dureetv2"/>
                            <field name="emissiontv"/>
                            <field name="ranglistetele"/>
                            <field name="timeslotprixtele"/>
                            <field name="prixtv"/>
                            <field name="description"/>
                        </group>                       
                    </sheet>
                </form>
            </field>
        </record>

<!-- VIEW SEARCH LISTE TELE -->

        <record model="ir.ui.view" id="liste_tele_search_view">
            <field name="name">listetele.search</field>
            <field name="model">liste.tv</field>
            <field name="arch" type="xml">
                <search>
                    <filter name="chainelistegroup" string="CHAINE" context="{'group_by':'name'}"/>
                    <field name="name"/>
                    <field name="emissiontv"/>
                    <field name="dureetv1"/>
                    <field name="dureetv2"/>
                </search>
            </field>
        </record>

<!-- VIEW TREE LISTE TELE -->

        <record model="ir.ui.view" id="liste_tele_tree_view">
            <field name="name">listetele.tree</field>
            <field name="model">liste.tv</field>
            <field name="arch" type="xml">
                <tree string="Course Tree">
                    <field name="name"/>
                    <field name="emissiontv"/>
                    <field name="dureetv1"/>
                    <field name="dureetv2"/>
                    <field name="prixtv"/>
                </tree>
            </field>
        </record>

<!-- ACTION LISTE TELE -->
        <!--
            The following tag is an action definition for a "window action",
            that is an action opening a view or a set of views
        -->
        <record model="ir.actions.act_window" id="liste_tele_action">
            <field name="name">LISTE TELE</field>
            <field name="res_model">liste.tv</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_chainelistegroup':1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Créez votre liste de prix TV ici.
                </p>
            </field>
        </record>


<!-- MENU REHETRA -->
        <!-- top level menu: no parent -->
        <menuitem id="main_tele_menu" name="Monitoring TV" sequence="3"/>
        <!-- A first level in the left side menu is needed
             before using action= attribute -->
        <menuitem id="pige_tele_menu" name="Monitoring TV"
                  parent="main_tele_menu"/>

        <menuitem id="tele_telephonie" name="ANALYSE"
                  parent="main_tele_menu"/>

        <menuitem id="tele_boisson" name="SECTEUR BOISSON"
                  parent="main_tele_menu"/>
<!--
        <menuitem id="tele_savonnerie" name="SECTEUR SAVONNERIE"
                  parent="main_tele_menu"/>

        <menuitem id="tele_libre" name="SECTEUR LIBRE"
                  parent="main_tele_menu"/>
-->
        <menuitem id="mediametrie_menu" name="Audience TV"
                  parent="main_audience_menu"/>

        <menuitem id="paramettretele_menu" name="PARAMETTRE" sequence="50"
                  parent="main_tele_menu"/>
<!-- MENU AVEC ACTION -->
        <menuitem id="liste_tele_menu" name="LISTE PRIX TV" sequence="50" parent="paramettretele_menu"
                  action="liste_tele_action"/>
        <!-- Full id location:
             action="openacademy.course_list_action"
             It is not required when it is the same module -->

<!-- BRUTE  TELE  form view -->
        <record model="ir.ui.view" id="brute_tele_form_view">
            <field name="name">brutetele.form</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <form string="Brute TELE">
                    <sheet>
                            <group string="Resultat de pige">
                                <field name="dayofdatetv"/>
                                <field name="date"/>
                                <field name="name"/>
                                <field name="categorytele"/>
                                <field name="secteurtele"/>
                                <field name="societetele"/>
                                <field name="typetele"/>
                                <field name="emissiontele"/>
                                <field name="campagnetele"/>
                                <field name="versiontele"/>
                                <field name="rangtele"/>
                                <field name="timeslotgrp"/>
                                <field name="dureetele"/>
                            </group>
                            <group string="Valeurs">
                                <field name="prixtele"/>
                                <field name="grpens"/>
                                <field name="grpcsp"/>
                                <field name="grppopact"/>
                                <field name="grpjeune"/>
                                <field name="grpmf"/>
                            </group>
                    </sheet>
                        <group>
                            <field name="moisgrp"/>
                            <field name="dayforgrp"/>
                            <field name="annee"/>
                            <field name="timeofdatetv"/>
                            <field name="timeslottele"/>
                        </group>
                </form>
            </field>
        </record>

<!-- SEARCH VIEW BRUTE TELE -->

        <record model="ir.ui.view" id="brute_tele_search_view">
            <field name="name">brutetele.search</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <search>
                    <field name="campagnetele"/>
                    <field name="societetele"/>
                    <field name="name"/>
                    <field name="categorytele"/>
                    <field name="secteurtele"/>
                    <field name="emissiontele"/>
                    <filter string="Ce mois-ci" name="current_month" domain="[('date','&lt;',(context_today()+relativedelta(months=1)).strftime('%%Y-%%m-01')), ('date','&gt;=',time.strftime('%%Y-%%m-01'))]"/>
                    <filter string="Mois Dernier" name="prev_month" domain="[('date','&gt;=',(context_today()-relativedelta(months=1)).strftime('%%Y-%%m-01')),('date','&lt;',time.strftime('%%Y-%%m-01'))]"/>
                    <separator/>
                    <filter string="Semaine dernière" name="semainederniere" domain="[('date','&gt;=',((context_today()+relativedelta(weeks=-2,days=1,weekday=0)).strftime('%%Y-%%m-%%d'))),('date', '&lt;=', ((context_today()+relativedelta(weeks=-1,weekday=6)).strftime('%%Y-%%m-%%d')))]"/>
                    <separator/>
                    <filter name="chainegroup" string="CHAINE" context="{'group_by':'name'}"/>
                    <filter name="sectorgroup" string="SECTEUR" context="{'group_by':'secteurtele'}"/>
                    <separator/>
                    <filter name="campagnegroup" string="CAMPAGNE" context="{'group_by':'campagnetele'}"/>
                    <filter name="societegroup" string="SOCIETE" context="{'group_by':'societetele'}"/>
                    <filter name="spot" string="SPOT" domain="[('typetele','=','SPOT')]"/>
                    <filter name="billboardavant" string="Billboard" domain="[('typetele','!=','SPOT')]"/>
                    <separator/>
                    <filter name="telephonie" string="TELECOMMUNICATION" domain="[('categorytele','=','TELECOMMUNICATION')]"/>
                    <filter name="tv" string="TV" domain="[('categorytele','=','TV')]"/>
                    <filter name="boissons" string="BOISSONS" domain="[('categorytele','=','BOISSONS')]"/>
                    <filter name="produitlaitier" string="PRODUIT LAITIER" domain="[('secteurtele','=','PRODUIT LAITIER')]"/>
                    <filter name="nettoyant" string="NETTOYANT" domain="[('categorytele','=','NETTOYANT')]"/>
                </search>
            </field>
        </record>

<!-- BRUTE TELE tree/list view -->
        <record model="ir.ui.view" id="brute_tele_tree_view">
            <field name="name">brutetele.tree</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <tree string="Brute TV Tree">
                    <field name="date"/>
                    <field name="name"/>
                    <field name="categorytele"/>
                    <field name="secteurtele"/>
                    <field name="societetele"/>
                    <field name="typetele"/>
                    <field name="emissiontele"/>
                    <field name="campagnetele"/>
                    <field name="grpens"/>
                    <field name="grpcsp"/>
                    <field name="grppopact"/>
                    <field name="grpjeune"/>
                    <field name="grpmf"/>
                    <field name="prixtele"/>
                </tree>
            </field>
        </record>
<!-- BRUTE GRAPH VIEW-->
        <record model="ir.ui.view" id="brute_tele_graph_view">
            <field name="name">brutetele.graph</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <graph string="Analyse">
                    <field name="societetele" type="row"/>
                    <field name="prixtele" type="measure"/>
                </graph>
            </field>
        </record>

<!-- PIVOT VIEW -->
        <record model="ir.ui.view" id="brute_tele_pivot_view">
            <field name="name">brutetele.pivot</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <pivot string="TCD">
                    <field name="societetele" type="row"/>
                    <field name="datesearch" type="col"/>
                    <field name="prixtele" type="measure"/>
                </pivot>
            </field>
        </record>

<!-- calendar view -->

        <record model="ir.ui.view" id="brute_tele_calendar_view">
            <field name="name">brutetele.calendar</field>
            <field name="model">brute.tele</field>
            <field name="arch" type="xml">
                <calendar string="Plan média" date_start="date" date_stop="date" mode="month"
                          color="campagnetele">
                    <field name="campagnetele"/>
                </calendar>
            </field>
        </record>

<!-- ACTION BRUTE -->

        <record model="ir.actions.act_window" id="brute_tele_list_action">
            <field name="name">BRUTE TV</field>
            <field name="res_model">brute.tele</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_mode">tree,form,graph,calendar,search</field>
            <field name="context">{'search_default_campagnegroup':1,'search_default_spot':1,'search_default_semainederniere':1}</field>
        </record>

        <menuitem id="brute_tele_menu" name="BRUTE TV" sequence="0"
                  parent="pige_tele_menu"
                  action="brute_tele_list_action"/>

